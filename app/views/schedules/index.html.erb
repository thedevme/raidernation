<% @schedules.each do |schedule| %>
  <tr>
    <td><%= schedule.game_tv %></td>
    <td><%= schedule.game_score %></td>
    <td><%= schedule.game_key %></td>
    <td><%= schedule.status %></td>
    <td><%= schedule.week %></td>
