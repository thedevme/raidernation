    <td><%= schedule.date_time_est %></td>
    <td><%= schedule.day_of_week %></td>
    <td><%= schedule.month_of_year %></td>
    <td><%= schedule.time %></td>
    <td><%= schedule.type %></td>
    <td><%= schedule.nfl_type %></td>
    <td><%= schedule.is_current_or_next_game %></td>
    <td><%= schedule.title %></td>
    <td><%= schedule.home_team_name %></td>
    <td><%= schedule.home_team_short_name %></td>
    <td><%= schedule.home_team_abbr %></td>
    <td><%= schedule.home_team_id %></td>
    <td><%= schedule.home_team_small_helmet %></td>
    <td><%= schedule.home_team_large_helmet %></td>
    <td><%= schedule.visitor_team_name %></td>
    <td><%= schedule.visitor_team_short_name %></td>
    <td><%= schedule.visitor_team_abbr %></td>
    <td><%= schedule.visitor_team_id %></td>
    <td><%= schedule.visitor_team_small_helmet %></td>
    <td><%= schedule.visitor_team_large_helmet %></td>
    <td><%= schedule.opponent_team_name %></td>
    <td><%= schedule.opponent_team_short_name %></td>
    <td><%= schedule.game_played_at %></td>
    <td><%= schedule.outcome %></td>
    <td><%= schedule.visitor_team_point_total %></td>
    <td><%= schedule.visitor_team_point_q1 %></td>
    <td><%= schedule.visitor_team_point_q2 %></td>
    <td><%= schedule.visitor_team_point_q3 %></td>
    <td><%= schedule.visitor_team_point_q4 %></td>
    <td><%= schedule.visitor_team_point_ot %></td>
    <td><%= schedule.home_team_point_total %></td>
    <td><%= schedule.home_team_point_q1 %></td>
    <td><%= schedule.home_team_point_q2 %></td>
    <td><%= schedule.home_team_point_q3 %></td>
    <td><%= schedule.home_team_point_q4 %></td>
    <td><%= schedule.home_team_point_ot %></td>
    <td><%= schedule.score %></td>
    <td><%= schedule.phase %></td>
    <td><%= schedule.clock %></td>
    <td><%= schedule.is_tie %></td>
    <td><%= schedule.winner_team_id %></td>
    <td><%= schedule.pre_game_link %></td>
    <td><%= schedule.post_game_link %></td>
    <td><%= link_to 'Show', schedule %></td>
    <td><%= link_to 'Edit', edit_schedule_path(schedule) %></td>
    <td><%= link_to 'Destroy', schedule, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

